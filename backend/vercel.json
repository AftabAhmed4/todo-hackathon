{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "BETTER_AUTH_SECRET": "@better_auth_secret",
    "GEMINI_API_KEY": "@gemini_api_key",
    "FRONTEND_URL": "https://todo-hackathon-frontend-phi.vercel.app"
  }
}